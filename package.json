{
  "name": "inventory-pro",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3000",
    "dev:test": "next dev . -p 3007",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:selenium": "tsx tests/selenium/smoke.test.ts",
    "test:selenium:crud": "vitest run tests/selenium/e2e-crud.test.ts tests/selenium/uom-flow.test.ts --maxWorkers=1 --reporter=junit --outputFile=test-results/selenium-junit.xml",
    "test:e2e:ui": "playwright test --ui",
    "test:integration": "vitest run tests/integration --fileParallelism=false",
    "test:unit": "vitest run tests/unit",
    "test:all": "bun run test:unit && bun run test:integration && bun run test:e2e",
    "test:full": "scripts/run-full-tests.cmd",
    "postinstall": "prisma generate",
    "test:ethel": "vitest run tests/selenium/ethel-test-suite.test.ts --maxWorkers=1",
    "demo:seed": "tsx scripts/demo/seed-demo-data.ts",
    "demo:record": "playwright test --config=playwright.config.demo.ts",
    "demo:record:login": "playwright test --config=playwright.config.demo.ts tests/demos/01-authentication",
    "demo:record:dashboard": "playwright test --config=playwright.config.demo.ts tests/demos/02-dashboard",
    "demo:record:products": "playwright test --config=playwright.config.demo.ts tests/demos/03-products",
    "demo:record:pos": "playwright test --config=playwright.config.demo.ts tests/demos/06-sales-workflow",
    "demo:process": "tsx scripts/post-production/process-demo.ts",
    "demo:process:login": "tsx scripts/post-production/process-demo.ts 01-authentication",
    "demo:burn-subs": "tsx scripts/post-production/burn-subtitles.ts",
    "demo:generate-tts": "tsx scripts/post-production/generate-tts.ts",
    "demo:tts:free": "tsx scripts/post-production/generate-tts-free.ts",
    "demo:tts:login": "tsx scripts/post-production/generate-tts-free.ts demo-recordings/01-authentication/login-script.json demo-recordings/01-authentication/login-audio.mp3",
    "db:check": "node scripts/pre-migration-checks.js",
    "db:compare": "node scripts/compare-neon-schemas.js",
    "db:deploy:prod": "powershell -ExecutionPolicy Bypass -File scripts/deploy-to-production.ps1",
    "db:sync:verify": "npx prisma migrate status && node scripts/compare-neon-schemas.js",
    "git:sync": "powershell -ExecutionPolicy Bypass -File scripts/auto_git_sync.ps1",
    "cap:init": "npx cap init InventoryPro com.inventorypro.app --web-dir out",
    "cap:sync": "npx cap sync",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npx cap run ios",
    "cap:run:android": "npx cap run android",
    "build:mobile": "cross-env NEXT_CONFIG=mobile next build --config next.config.mobile.ts",
    "mobile:android": "bun run build:mobile && bun run cap:sync && bun run cap:open:android",
    "mobile:ios": "bun run build:mobile && bun run cap:sync && bun run cap:open:ios"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/barcode-scanner": "^3.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@simplewebauthn/browser": "^13.2.2",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-query-next-experimental": "^5.91.0",
    "@types/uuid": "^10.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "edge-tts-node": "^1.5.7",
    "face-api.js": "^0.22.2",
    "fluent-ffmpeg": "^2.1.3",
    "html2canvas": "^1.4.1",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.468.0",
    "next": "^16.0.10",
    "pg": "^8.16.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.1",
    "react-barcode": "^1.6.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.66.0",
    "react-to-print": "^3.2.0",
    "recharts": "^2.15.4",
    "say": "^0.16.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/selenium-webdriver": "^4.35.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "prisma": "^7.1.0",
    "selenium-webdriver": "^4.38.0",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vercel": "^50.1.3",
    "vitest": "^4.0.8"
  }
}
